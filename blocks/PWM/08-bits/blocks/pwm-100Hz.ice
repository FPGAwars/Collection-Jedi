            {
              "id": "2c9137da-f6a5-4e09-8623-51b395cc727d",
              "type": "basic.code",
              "data": {
                "code": "//-- Parámetro N: numero de bits  \n//-- del prescaler\n\n//-- El prescaler es un contador del sistema  \n//-- de N bits\n\nreg [N-1:0] pres = 0;\n\nalways @(posedge clk)\n  pres <= pres + 1;\n  \n//-- La salida de reloj dividida es el bit  \n//-- de mayor peso\nassign clk_div = pres[N-1];\n\n//-- La salida de overflow es el tic que  \n//-- se obtiene en el flanco de bajada del  \n//-- bit de mayor peso (el de salida)\n\nreg q = 0;\nalways @(posedge clk)\n  q <= clk_div;\n  \nassign ov = (q & ~clk_div);  ",
                "params": [
                  {
                    "name": "N"
                  }
                ],
                "ports": {
                  "in": [
                    {
                      "name": "clk"
                    }
                  ],
                  "out": [
                    {
                      "name": "clk_div"
                    },
                    {
                      "name": "ov"
                    }
                  ]
                }
              },
